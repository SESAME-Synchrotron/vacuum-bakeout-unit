record(bi, "HEATER2:isEnabled")
{
 	field(DESC, "is the heater enabled")
	field(DTYP, "s7nodave")
	field(INP,  "@bakeout-plc (PG=digital) DB10.DBX59.3 bool")
	field(PINI, "YES")
	field(ZNAM, "DISABLED") 	field(ZSV, "NO_ALARM")
	field(ONAM, "ENABLED") field(OSV, "NO_ALARM")
	field(SCAN, "I/O Intr")
}

