record(ai, "HEATER2:getTemp")

{
	field(DESC, "read thermocuople temperature")
	field(DTYP, "s7nodave")
	field(INP,  "@bakeout-plc (PG=analog) DB10.DBD46 float")
	field(EGU,  "C")
